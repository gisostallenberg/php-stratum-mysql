/**
 * Test for designation type row0 with BLOB.
 *
 * @param p_count The number of rows selected.
 *                * 0 For a valid test.
 *                * 1 For a valid test.
 *                * 2 For a invalid test.
 * @param p_blob  The BLOB.
 *
 * @type row0
 */
create procedure tst_test_row0a_with_lob( p_count int,
                                          p_blob  blob )
reads sql data
begin
  select *
  from   TST_FOO2
  where  tst_c00 <= p_count
  ;
end
